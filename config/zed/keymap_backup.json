[
  {
    "bindings": {
      "cmd-g cmd-h": "pane::SplitLeft",
      "cmd-g cmd-l": "pane::SplitRight"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "space h": "workspace::ActivatePreviousPane",
      "space l": "workspace::ActivateNextPane"
    },
    "context": "Workspace"
  },
  {
    "context": "Workspace",
    "bindings": {
      "space e": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "VimControl",
    "bindings": {
      "shift-h": "pane::ActivatePrevItem",
      "shift-l": "pane::ActivateNextItem"
    }
  }
  {
    "context": "Workspace",
    "bindings": {
      "cmd-`": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-e": "workspace::ToggleRightDock"
    }
  },
  {
    "bindings": {
      "shift shift": "file_finder::Toggle"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "j k": [
        "workspace::SendKeystrokes",
        "escape"
      ]
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "g d": "editor::GoToDefinition"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": true
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": true
        }
      ]
    },
    "context": "VimControl"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": false
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": false
        }
      ]
    },
    "context": "VimCount"
  },
  {
    "bindings": {
      "shift-s": [
        "vim::PushAddSurrounds",
        {}
      ]
    },
    "context": "vim_mode == visual"
  }
]
