[
  {
    "bindings": {
      "cmd-alt-c": "workspace::CopyRelativePath"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "cmd-g cmd-j": "editor::GoToHunk",
      "cmd-g cmd-k": "editor::GoToPreviousHunk"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-t t": "task::Spawn"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-g alt-b": "git::Blame"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-g cmd-h": "pane::SplitLeft",
      "cmd-g cmd-l": "pane::SplitRight"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "space h": "workspace::ActivatePreviousPane",
      "space l": "workspace::ActivateNextPane"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "space e": "project_panel::ToggleFocus"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "shift-h": "pane::ActivatePreviousItem",
      "shift-l": "pane::ActivateNextItem"
    },
    "context": "VimControl"
  },
  {
    "bindings": {
      "cmd-`": "terminal_panel::ToggleFocus"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "ctrl-e": "workspace::ToggleRightDock"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "shift shift": "file_finder::Toggle"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "j k": [
        "workspace::SendKeystrokes",
        "escape"
      ]
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "g d": "editor::GoToDefinition"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-alt-c": "workspace::CopyPath"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "shift-<": "editor::Indent",
      "shift->": "editor::Outdent",
      "shift-j": "editor::MoveLineDown",
      "shift-k": "editor::MoveLineUp",
      "space c": "editor::ToggleComments"
    },
    "context": "vim_mode == visual"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": true
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": true
        }
      ]
    },
    "context": "VimControl"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": false
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": false
        }
      ]
    },
    "context": "VimCount"
  },
  {
    "bindings": {
      "shift-s": [
        "vim::PushAddSurrounds",
        {}
      ]
    },
    "context": "vim_mode == visual"
  },
  {
    "bindings": {
      "alt-b": [
        "task::Spawn",
        {
          "task_name": "Find bookmark"
        }
      ]
    }
  }
]
