[
  {
    "bindings": {
      "cmd-alt-c": "workspace::CopyRelativePath"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "cmd-g cmd-j": "editor::GoToHunk",
      "cmd-g cmd-k": "editor::GoToPreviousHunk"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-t t": "task::Spawn"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-g alt-b": "git::Blame"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "space h": "workspace::ActivatePreviousPane",
      "space l": "workspace::ActivateNextPane"
    },
    "context": "Editor && vim_mode == normal"
  },
  {
    "bindings": {
      "g d": "editor::GoToDefinition",
      "space c l": "editor::ToggleComments",
      "space e": "project_panel::ToggleFocus",
      "space f S": "workspace::SaveAll",
      "space f s": "workspace::Save",
      "space q a": "pane::CloseAllItems",
      "space q o": "pane::CloseInactiveItems",
      "space q q": "zed::Quit",
      "space q w": "workspace::CloseWindow",
      "space s H": "workspace::SwapPaneLeft",
      "space s J": "workspace::SwapPaneUp",
      "space s K": "workspace::SwapPaneDown",
      "space s L": "workspace::SwapPaneRight",
      "space s h": "pane::SplitLeft",
      "space s j": "pane::SplitDown",
      "space s k": "pane::SplitUp",
      "space s l": "pane::SplitRight",
      "space space": "command_palette::Toggle",
      "space t i": "editor::ToggleInlayHints",
      "space t n": "workspace::NewTerminal",
      "space t t": "terminal_panel::ToggleFocus",
      "space w w": "workspace::ActivateNextPane"
    },
    "context": "Editor && vim_mode == normal && (vim_operator == none || vim_operator == n) && !VimWaiting"
  },
  {
    "bindings": {
      "shift-h": "pane::ActivatePreviousItem",
      "shift-l": "pane::ActivateNextItem"
    },
    "context": "VimControl"
  },
  {
    "bindings": {
      "cmd-`": "terminal_panel::ToggleFocus"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "ctrl-e": "workspace::ToggleRightDock"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "shift shift": "file_finder::Toggle"
    },
    "context": "Workspace"
  },
  {
    "bindings": {
      "j k": [
        "workspace::SendKeystrokes",
        "escape"
      ]
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "g d": "editor::GoToDefinition"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "cmd-alt-c": "workspace::CopyPath"
    },
    "context": "Editor"
  },
  {
    "bindings": {
      "shift-<": "editor::Indent",
      "shift->": "editor::Outdent",
      "shift-j": "editor::MoveLineDown",
      "shift-k": "editor::MoveLineUp",
      "space c": "editor::ToggleComments"
    },
    "context": "vim_mode == visual"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": true
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": true
        }
      ]
    },
    "context": "VimControl"
  },
  {
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": false
        }
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": false
        }
      ]
    },
    "context": "VimCount"
  },
  {
    "bindings": {
      "shift-s": [
        "vim::PushAddSurrounds",
        {}
      ]
    },
    "context": "vim_mode == visual"
  },
  {
    "bindings": {
      "alt-b": [
        "task::Spawn",
        {
          "task_name": "Find bookmark"
        }
      ]
    }
  },
  {
    "bindings": {
      "space m 0": [
        "workspace::SendKeystrokes",
        "mJ"
      ],
      "space m 1": [
        "workspace::SendKeystrokes",
        "mA"
      ],
      "space m 2": [
        "workspace::SendKeystrokes",
        "mB"
      ],
      "space m 3": [
        "workspace::SendKeystrokes",
        "mC"
      ],
      "space m 4": [
        "workspace::SendKeystrokes",
        "mD"
      ],
      "space m 5": [
        "workspace::SendKeystrokes",
        "mE"
      ],
      "space m 6": [
        "workspace::SendKeystrokes",
        "mF"
      ],
      "space m 7": [
        "workspace::SendKeystrokes",
        "mG"
      ],
      "space m 8": [
        "workspace::SendKeystrokes",
        "mH"
      ],
      "space m 9": [
        "workspace::SendKeystrokes",
        "mI"
      ]
    },
    "context": "Editor && vim_mode == normal && vim_operator == none && !VimWaiting"
  }
]
