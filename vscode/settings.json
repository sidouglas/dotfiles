  "openInGitHub.defaultBranch": "master",
  "periscope.alwaysShowRgMenuActions": true,
  "periscope.rgQueryParams": [
    {
      "param": "-g '**/*$1*/**' -g '!**/node_modules/**'",
      "regex": "^(.+) -m ([\\w-_]+)$"
    },
    {
      "param": "-g '**/*$1*/**/*.$2'",
      "regex": "^(.+) -m ([\\w-_]+) ([\\w]+)$"
    },
    {
      "param": "-g '$1'",
      "regex": "^(.+) -g (.+)$"
    },
    {
      "param": "-t $1",
      "regex": "^(.+) -t ?(\\w+)$"
    },
    {
      "param": "-g '*.$1'",
      "regex": "^(.+) \\*\\.(\\w+)$"
    }
  ],
  "php-cs-fixer.autoFixByBracket": true,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
